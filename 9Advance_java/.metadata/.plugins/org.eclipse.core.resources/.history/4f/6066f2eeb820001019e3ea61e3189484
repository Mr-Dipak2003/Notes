package OOP;

import java.util.Scanner;

public class functionCalling {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter a number");
		int  num=sc.nextInt();
		Calc cal = new Calc();
		cal.getData(num);
		cal.showData();

	}

}
class Calc{
	int no;
	
	void getData(int x) {
		no=x;
	}
	void showData() {
		 System.out.println(num1 + num2);
	}
}
